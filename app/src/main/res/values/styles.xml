<resources>

    <!-- Base application theme. -->
    <style name="Forest" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryColor_Forest</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor_Forest</item>
        <item name="colorAccent">@color/secondaryColor_Forest</item>
        <item name="android:windowBackground">@color/secondaryColor_Forest</item>
    </style>

    <style name="Beach" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/primaryColor_Beach</item>
        <item name="colorPrimaryDark">@color/primaryDarkColor_Beach</item>
        <item name="colorAccent">@color/secondaryColor_Beach</item>
        <item name="android:windowBackground">@color/secondaryColor_Beach</item>
    </style>

</resources>
